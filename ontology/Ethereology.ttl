                                        rdfs:subPropertyOf <http://ethon.consensys.net/Consensus_Object_Property> ;
                                        rdfs:domain <http://ethon.consensys.net/Block> ;
                                        rdfs:range <http://ethon.consensys.net/ProtocolVariant> ;
                                        <http://ethon.consensys.net/suggestedStringRepresentation> "conformsTo" ;
                                        rdfs:comment "Relates a Block to the protocol variant it conforms to."@en ;
                                        rdfs:label "conforms to" ;
                                        ns:term_status "unstable" .


